<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="phpmob.command.ace_installer" class="PhpMob\AceBundle\Command\AceInstallerCommand">
            <argument type="service" id="fos_ck_editor.installer" />
            <tag name="console.command" command="phpmob:ckeditor:install" />
        </service>
        <service id="phpmob.form.ace_type" class="PhpMob\AceBundle\Form\Type\AceType">
            <tag name="form.type"/>
        </service>
    </services>
</container>
